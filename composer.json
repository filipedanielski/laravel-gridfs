{
    "name": "filipedanielski/gridfs",
    "description": "Use GridFS on Laravel to store your files, images and documents",
    "keywords": ["laravel","gridfs","mongodb","mongo","database"],
    "homepage": "https://github.com/filipedanielski/laravel-gridfs",
    "type": "library",
    "authors": [
        {
            "name": "Filipe Danielski Andrade",
            "homepage": "https://filipedanielski.com.br"
        }
    ],
    "license" : "MIT",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "^2.9",
        "illuminate/container": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/support": "^8.0",
        "jenssegers/mongodb": "^3.8",
        "maennchen/zipstream-php": "^1.2.0 || ^2.0.0",
        "mongodb/mongodb": "^1.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Filipedanielski\\Gridfs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filipedanielski\\Gridfs\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
