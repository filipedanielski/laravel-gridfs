{
    "name": "filipedanielski/gridfs",
    "description": "Use GridFS on Laravel to store your files, images and documents",
    "keywords": ["laravel","gridfs","mongodb","mongo","database"],
    "homepage": "https://github.com/filipedanielski/laravel-gridfs",
    "authors": [
        {
            "name": "Filipe Danielski Andrade",
            "homepage": "https://filipedanielski.com.br"
        }
    ],
    "license" : "MIT",
    "require": {
        "php": ">=7.0.0",
        "jenssegers/mongodb": "^3.3",
        "illuminate/support": "^5.5",
        "illuminate/container": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/events": "^5.5",
        "mongodb/mongodb": "^1.0.0",
        "maennchen/zipstream-php": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "^3.1",
        "mockery/mockery": "^0.9",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Filipedanielski\\Gridfs\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    }
}
